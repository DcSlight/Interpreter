$a$ (x) => 1+x
$b$ (y) => @a{y}
@b{5}

$c$ (z,y) => z * @b{y}
@c{3,4}

$sumDigits$ (number) => (number==0) || (number%10 + @sumDigits{number/10})
(@sumDigits{12345}) - 1

## Factorial Function - @factorial{5}
$factorial$ (n) => (n==0) || (n*@factorial{n-1})
@factorial{5}
## Fibonacci Function - @fib{8}
$fib$ (n) => n == 1 || n == 2 || @fib{n - 1} + @fib{n - 2}
@fib{8}
